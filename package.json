{
  "name": "saturn",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/db/migrate.js && node dist/db/seed.js && node dist/index.js",
    "seed": "tsx src/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "test:e2e:legacy": "./scripts/e2e-test.sh",
    "test:e2e:sdk": "tsx scripts/e2e-test-sdk.ts",
    "test:capabilities": "tsx scripts/test-capabilities.ts",
    "pricing:update": "tsx scripts/update-pricing.ts",
    "pricing:preview": "tsx scripts/update-pricing.ts --dry-run"
  },
  "keywords": [],
  "author": "",
  "license": "BUSL-1.1",
  "description": "Lightning-powered API proxy for AI agents",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@sentry/node": "^10.38.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "ln-service": "^57.27.3",
    "node-cron": "^4.2.1",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "stripe": "^20.3.0",
    "ulid": "^3.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.8",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
