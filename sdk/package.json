{
  "name": "@saturn-pay/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Saturn payment proxy API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
