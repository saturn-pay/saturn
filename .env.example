# Server
PORT=3000
NODE_ENV=development

# Database
DATABASE_URL=postgresql://saturn:saturn@localhost:5432/saturn

# LND (Voltage or self-hosted)
# Use a restricted macaroon with only: invoices:read invoices:write info:read
# Bake with: lncli bakemacaroon invoices:read invoices:write info:read
LND_SOCKET=yournode.m.voltageapp.io:10009
LND_TLS_CERT=base64_encoded_tls_cert
LND_MACAROON=base64_encoded_restricted_macaroon

# Upstream API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
SERPER_API_KEY=...
FIRECRAWL_API_KEY=...
E2B_API_KEY=...
JINA_API_KEY=...
BRAVE_SEARCH_API_KEY=...
RESEND_API_KEY=re_...
TWILIO_ACCOUNT_SID=...
TWILIO_AUTH_TOKEN=...
TWILIO_FROM_NUMBER=...
REPLICATE_API_TOKEN=...
ELEVENLABS_API_KEY=...
DEEPGRAM_API_KEY=...
SCRAPERAPI_API_KEY=...
HUNTER_API_KEY=...
PINECONE_API_KEY=...
PINECONE_ENVIRONMENT=...

# Stripe
STRIPE_SECRET_KEY=sk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...

# CORS
CORS_ORIGIN=https://yourdomain.com

# Error Tracking (optional)
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# E2E Testing (optional)
E2E_API_URL=https://api.saturn-pay.com
E2E_DASHBOARD_URL=https://app.saturn-pay.com
STRIPE_TEST_SECRET_KEY=sk_test_...
